<div id="myModalcandidateedit" class="modal fade" role="dialog">
  <div class="modal-dialog">
    <div class="modal-content text-center">
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal">&times;</button>
        <h4 id = "candidatetitle" class="modal-title">Edit Candidate <%= @candidate.id %> </h4>
      </div>
      <div id = "candidateerrors"></div>
      <div class="modal-body div-centered">
        
        <%= render 'candidates/form', candidate: @candidate %>

      <div class="modal-footer">
      </div>
    </div>
  </div>
</div>